{
    "moonrakerHost" : "localhost",
    "moonrakerPort": 7125,
    "apiTimeout": 2,
    "updateInterval": 15,
    "debug" : false,
    "taskList": [
      {
        "command": "/gutenberg/apps/klipperPenguinBridge/script/get-if-ip-addr.py eth0",
        "execTimeout": 1,
        "variableName": "eth0_ip",
        "variableType" : "string"
      },
      {
        "command": "/gutenberg/apps/klipperPenguinBridge/script/get-if-ip-addr.py wlan0",
        "execTimeout": 1,
        "variableName": "wlan0_ip",
        "variableType" : "string"
      }
    ]
  }
  
